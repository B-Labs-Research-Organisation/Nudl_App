{
  "name": "discord-web3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "preinstall": "npx only-allow pnpm",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write src",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format:check": "prettier src --check",
    "fix": "pnpm format && pnpm lint",
    "watch": "tsc -b --watch",
    "husky": "husky install",
    "e2e": "ts-node  -r tsconfig-paths/register node_modules/tape/bin/tape src/**/*.e2e.ts",
    "test": "ts-node -r tsconfig-paths/register node_modules/tape/bin/tape src/**/*.test.ts src/*.test.ts src/**/**/*.test.ts",
    "prepare": "ts-patch install && typia patch",
    "pretest": "ts-patch install",
    "start": "ts-node -r tsconfig-paths/register src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^2.4.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/redis": "^4.0.11",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-bearer-token": "^3.0.0",
    "lodash": "^4.17.21",
    "redis": "^4.7.0",
    "typia": "^7.6.1",
    "viem": "^2.23.5"
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "tape": "^5.9.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "~5.7.3"
  }
}